<UserControl x:Class="WPF样式收集.Pages.菜单折叠效果.RoundMenuControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPF样式收集.Pages.菜单折叠效果">
    <Canvas x:Name="mainCanvas">
        <local:MyButton x:Name="MenuBtn" Canvas.Left="200" Canvas.Top="200" Width="32" Height="32" DisplayImage="/Resources/add.png" Style="{StaticResource MenuButtonStyle}" Click="MenuBtn_Click" Panel.ZIndex="1"></local:MyButton>
    </Canvas>
</UserControl>
